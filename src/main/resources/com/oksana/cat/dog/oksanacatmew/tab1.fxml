<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.collections.FXCollections?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="box1" prefHeight="692.0" prefWidth="514.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.oksana.cat.dog.oksanacatmew.MainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Label fx:id="welcomeText"  />
    <Label fx:id="welcomeText2" />
    <ComboBox fx:id="comboCow" layoutX="15.0" layoutY="33.0" prefWidth="90.0">
        <items>
            <FXCollections fx:factory="observableArrayList">
                <String fx:value="Dry cow" />
                <String fx:value="Lactation cow" />
            </FXCollections>
        </items>
    </ComboBox>

    <TextField fx:id="weight" prefHeight="26.0" prefWidth="75.0" promptText="weight" />
    <TextField fx:id="NEL_featus" prefHeight="26.0" prefWidth="146.0" promptText="NEL_featus (Dry cow only)" />
    <TextField fx:id="protein" prefHeight="26.0" prefWidth="169.0" promptText="crude protein (Dry cow only)" />
    <TextField fx:id="fibers" prefHeight="26.0" prefWidth="212.0" promptText="fibers" />
    <TextField fx:id="sugar" promptText="sugar" />
    <TextField fx:id="fat" promptText="fat" />
    <TextField fx:id="calcium" promptText="calcium" />
    <TextField fx:id="phos" promptText="phos" />
    <TextField fx:id="mp" promptText="milk production (Lactating cow only)" />
    <TextField fx:id="mf" promptText="milk fat (Lactating cow only)" />
    <TextField fx:id="mpt" promptText="milk protein (Lactating cow only)" />
    <Button onAction="#onCalculateButtonClick" text="Calculate" />
</VBox>
